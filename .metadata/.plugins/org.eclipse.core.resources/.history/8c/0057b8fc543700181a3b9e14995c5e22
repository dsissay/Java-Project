package com.JavaIO.demo;

import java.io.File;
import java.io.IOException;

public class FileManagment {
	public static void main (String [] args) throws IOException
	{
		 CreateFile fl=new CreateFile();
		 fl.fileCreated();
		
		 fileWritter();
	myFileReader();
	}

	private static void myFileReader() {
		FileReader fr=new FileReader("C:\\Users\\Daniel\\Desktop\\myFile.txt");
		fr.myFileReader();
		
	}

	public static void fileWritter() {
		WriteContentOnFile wt= new WriteContentOnFile();
		wt.fileWritten();
	}
}
