package com.collaborait.demo;

public class JavaLoops {
	public static void main(String[] args) {
		//we have four types of loops in java
		// simple for loop
			///	enhanced for each loop
			////while loop
			////do while loop
			////continue statments
			////break statments
	
		
		// Java for Loops
		int i=0;
		for (i=0;i<10; i++) {
			System.out.println(i);
			i+1;
		}
		
		
		
		
	}

}
