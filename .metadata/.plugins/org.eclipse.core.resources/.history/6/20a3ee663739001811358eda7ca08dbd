package com.JavaCollectionClasses.demo;

import java.util.*;

public class PriortyLst {
	
	public static void main (String[] args) {
		prtList();
		
	}

		public static void prtList() {
			
			Comparator<String> cp = new StringLengthComparator();
			PriorityQueue <String> pq= new PriorityQueue(10,cp);
			
			pq.add("short");
	        pq.add("very long indeed");
	        pq.add("medium");
	        
	        System.out.println(pq.remove());
	        
	        while (pq.size() != 0)
	        {
	            //System.out.println(pq.remove());
	        }
			
		}
	
	

}
